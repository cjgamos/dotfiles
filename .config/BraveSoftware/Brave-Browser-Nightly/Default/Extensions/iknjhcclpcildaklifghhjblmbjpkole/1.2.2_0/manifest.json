{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "images/icon16.png",
         "24": "images/icon24.png",
         "32": "images/icon32.png"
      },
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "contentScript.js" ],
      "matches": [ "https://marketplace.axieinfinity.com/axie/*" ]
   } ],
   "description": "This extension will help with some Axie damage calculations",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Flowbot44's Axie Infinity Extension",
   "permissions": [ "https://marketplace.axieinfinity.com/axie/*", "tabs", "webNavigation" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.2"
}
