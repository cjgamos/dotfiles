{
   "author": "Sky Mavis Engineering",
   "background": {
      "persistent": true,
      "scripts": [ "browser-polyfill.js", "background.js", "vendors.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "browser_specific_settings": {
      "gecko": {
         "id": "ronin-wallet@axieinfinity.com",
         "strict_min_version": "68.0"
      }
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Shift+Alt+R",
            "linux": "Shift+Alt+R",
            "mac": "Shift+Alt+R",
            "windows": "Shift+Alt+R"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "browser-polyfill.js", "content-script.js", "vendors.js" ],
      "matches": [ "*://*.axieinfinity.com/*", "*://*.skymavis.com/*", "*://*.roninchain.com/*", "*://*.axiedao.org/*", "*://*.skymavis.one/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "trezor-content-script.js" ],
      "matches": [ "*://connect.trezor.io/*/popup.html" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.mxpnl.com/; object-src 'self'",
   "description": "A wallet for Sky Mavis's Ronin sidechain",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Ronin Wallet",
   "permissions": [ "storage", "unlimitedStorage", "*://connect.trezor.io/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8.0",
   "web_accessible_resources": [ "in-page.js" ]
}
