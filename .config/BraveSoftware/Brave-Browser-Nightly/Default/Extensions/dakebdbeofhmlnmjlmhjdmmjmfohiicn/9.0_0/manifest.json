{
   "action": {
      "default_icon": {
         "24": "icons/icon24.png",
         "36": "icons/icon36.png",
         "48": "icons/icon48.png"
      },
      "default_popup": "popup.html",
      "default_title": "Google Meet Tweak"
   },
   "author": "Simone Marullo",
   "background": {
      "service_worker": "update_worker.js"
   },
   "content_scripts": [ {
      "js": [ "browser-polyfill.js", "content.js", "mutesync.js" ],
      "matches": [ "https://meet.google.com/*", "https://*.zoom.us/*", "https://*.zoomgov.com/*", "https://meet.google.com/*", "https://*.gotomeet.me/*", "https://*.gotowebinar.com/*" ],
      "run_at": "document_idle"
   }, {
      "css": [ "raw_styles.css" ],
      "js": [ "raw_content.js" ],
      "matches": [ "https://meet.google.com/*", "https://*.zoom.us/*", "https://*.zoomgov.com/*", "https://meet.google.com/*", "https://*.gotomeet.me/*", "https://*.gotowebinar.com/*" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "Send emojis, Text Overlay, Cam Effects and layout tweaks in Google Meet + record meetings with Zoomcorder",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "icons/icon128.png",
      "24": "icons/icon24.png",
      "36": "icons/icon36.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Google Meet Tweak (Emojis, Text, Cam Effects)",
   "options_page": "raw_options.html",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "9.00",
   "web_accessible_resources": [ {
      "matches": [ "https://meet.google.com/*" ],
      "resources": [ "grid.user.js", "cam_script.js", "/images/howto.jpg" ]
   }, {
      "matches": [ "https://*.zoom.us/*", "https://*.zoomgov.com/*", "https://meet.google.com/*", "https://*.gotomeet.me/*", "https://*.gotowebinar.com/*" ],
      "resources": [ "/images/record.png", "/images/recimage.png", "/images/howto.jpg", "/images/download.png" ]
   } ]
}
